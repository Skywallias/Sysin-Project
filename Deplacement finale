    Public Sub Deplacerrobot(ByVal trajet As List(Of Common_Robot.Point2D), ByVal k As Integer)

        Dim positionrobot As Common_Robot.Point2D = RetournerPosition()
        Dim ProchaineCase As Common_Robot.Point2D = trajet(trajet.Count - 1)

        RotationDemandee(ProchaineCase, positionrobot)

        Me.Avancer()
    End Sub
